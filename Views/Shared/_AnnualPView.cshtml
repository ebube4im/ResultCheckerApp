@model IEnumerable<GodlySeeds_Online_Result_Checker.Models.PrintAnnualViewModel>

<div class="col-12">
    <table style="margin-top:-10px" class="table table-hover table-striped table-bordered table-sm">
        <thead class="thead-light">
            <tr style="background-color:#4b94ee; color:white;">
                           
                <th>
                    @Html.DisplayNameFor(model => model.Subjectname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirstTermTotal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirstTermGrade)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.secondTermTotal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.secondTermGrade)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.thirdTermTotal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.thirdTermGrade)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TOTAL)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AVERAGE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CLASSAVERAGE)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.POSITION)
                </th>
            
            </tr>
</thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Subjectname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstTermTotal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstTermGrade)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.secondTermTotal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.secondTermGrade)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.thirdTermTotal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.thirdTermGrade)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TOTAL)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AVERAGE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CLASSAVERAGE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.POSITION)
                    </td>

                </tr>
            }
        </tbody>
    </table>
    </div>